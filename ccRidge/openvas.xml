<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>openvas</Name>
  <Description>A dockerized openvas.[br]&#13;

               [b]/data[/b]        : [br][br]&#13;
</Description>
  <Registry>https://registry.hub.docker.com/u/ccridge/openvas/</Registry>
  <Repository>ccridge/openvas</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9390</HostPort>
        <ContainerPort>9390</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9391</HostPort>
        <ContainerPort>9391</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/lib/openvas/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>d2fd3a10</Version>
  <WebUI></WebUI>
  <Banner>http://i.imgur.com/wpeOxB1.png</Banner>
  <Icon>http://i.imgur.com/AxkKWji.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
